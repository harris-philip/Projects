          <SubComponents>
            <Component class="javax.swing.JLabel" name="page1Text">
              <Properties>
                <Property name="font" type="java.awt.Font" editor="org.netbeans.beaninfo.editors.FontEditor">
                  <Font name="Calibri" size="14" style="0"/>
